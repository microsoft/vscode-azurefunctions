        "@types/opn": "^5.1.0",
        "@types/ps-tree": "^1.1.0",
        "@types/request": "2.0.7",
        "@types/request-promise": "4.1.38",
        "@types/semver": "^5.5.0",
        "@types/websocket": "^0.0.37",
        "@types/xml2js": "^0.4.2",
        "@types/xregexp": "^3.0.29",
        "azure-storage": "^2.8.1",
        "gulp": "^3.9.1",
        "gulp-decompress": "^2.0.1",
